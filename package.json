{
  "name": "swagger-data-generator",
  "version": "0.1.0-beta.7",
  "description": "Generate mock data for a swagger document.",
  "main": "dist/src/SwaggerDataGenerator",
  "types": "lib/index",
  "scripts": {
    "test": "ava",
    "build": "tsc -p .",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/ts-node -- ./index.ts",
    "dev": "./node_modules/ts-node -- ./index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricHenry/swagger-data-generator.git"
  },
  "keywords": [
    "swagger",
    "mock",
    "data",
    "generate",
    "generator",
    "openAPI"
  ],
  "author": "Eric Henry Correia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricHenry/swagger-data-generator/issues"
  },
  "homepage": "https://github.com/EricHenry/swagger-data-generator#readme",
  "bin": {
    "swagger-data-gen": "lib/index"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "json-schema-faker": "^0.3.1",
    "lodash": "^4.11.1",
    "swagger-parser": "^3.4.1"
  },
  "devDependencies": {
    "@types/argparse": "1.0.30",
    "@types/faker": "4.1.0",
    "@types/lodash": "4.14.64",
    "@types/node": "7.0.18",
    "ava": "^0.14.0",
    "nodemon": "1.11.0",
    "ts-node": "3.0.4",
    "typescript": "2.3.2"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "failFast": true,
    "tap": true
  }
}
